{
  "private": true,
  "scripts": {
    "prebuild": "rimraf build",
    "build": "cross-env NODE_ENV=production next build",
    "analyze": "cross-env ANALYZE=true yarn build",
    "dev": "cross-env NODE_ENV=development next dev --port 3030",
    "clean": "rimraf coverage build",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json && prettier --check \"**/*.{ts,tsx}\"",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/react-hooks": "^0.1.0-beta.7",
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.1",
    "@material-ui/system": "^4.0.1",
    "@types/ramda": "^0.26.18",
    "@webcarrot/xirr": "^1.0.3",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.3.1",
    "apollo-link": "^1.2.1",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.3",
    "apollo-utilities": "^1.3.0",
    "axios": "^0.19.0",
    "bignumber.js": "^9.0.0",
    "cross-env": "^5.2.0",
    "dotenv-extended": "^2.5.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.0",
    "highcharts": "^7.1.2",
    "highcharts-react-official": "^2.2.2",
    "hoist-non-react-statics": "^3.3.0",
    "material-table": "^1.40.1",
    "moment": "^2.24.0",
    "next-with-apollo": "^3.3.2",
    "ramda": "^0.26.0",
    "react": "^16.8.3",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.3",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^8.0.4-canary.18",
    "@types/jest": "^24.0.13",
    "@types/next": "^8.0.5",
    "@types/recharts": "^1.1.16",
    "babel-jest": "^24.8.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-environment-jsdom": "^24.8.0",
    "lint-staged": "^8.1.3",
    "next": "^9.3.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn typecheck && yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format",
      "git add"
    ]
  }
}
